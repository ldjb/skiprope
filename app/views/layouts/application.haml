!!!
%html
  %head
    %meta{charset: "utf-8"}/
    %title #{$skiprope["name"]}
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags
  %body
    = render partial: "partials/navbar"
    .container
      .row
        .col-md-2
        .col-md-8
          = yield
      .row
        .col-md-12
          = render partial: "partials/footer"
